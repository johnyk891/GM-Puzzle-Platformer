        <arguments>
          <argument>
            <kind>1</kind>
            <string>//Get the player's input
key_right = keyboard_check(ord('D'));
key_left = -keyboard_check(ord('A'));
key_jump = keyboard_check_pressed(vk_space);
key_pull = keyboard_check_released(ord('Q'));

//React to inputs
move = key_left + key_right;
hsp = move * movespeed;
if (vsp &lt; 10) vsp += grav;
 
if (place_meeting(x,y+1,obj_wall))
{
    vsp = key_jump * -jumpspeed
}

// Horizontal collision
if (place_meeting(x+hsp,y,obj_wall))
{
    while(!place_meeting(x+sign(hsp),y,obj_wall))
    {
        x += sign(hsp);
    }
    hsp = 0;
}
x += hsp;

//Vertical Collision
if (place_meeting(x,y+vsp,obj_wall))
{
    while(!place_meeting(x,y+sign(vsp),obj_wall))
    {
        y += sign(vsp);
    }
    vsp = 0;
}
y += vsp;
//Stairs Collision
if (place_meeting(x+1,y,obj_stair_main))
{
    if (!place_meeting(x,y-5,obj_box))
    {
        y-=5;
        x+=1;
    }
}
// Pull
if(key_pull!=0 &amp;&amp; pull == 0 &amp;&amp; distance_to_object(obj_box)&lt;10)
{
    box = instance_nearest(x,y,obj_box);
    pull = 1;
} 
else if(key_pull!=0 &amp;&amp; pull==1)
{
    pull = 0;
}

if(pull==1&amp;&amp;distance_to_object(box)&gt;60&amp;&amp;distance_to_object(box)&lt;100)
{
        movespeed = 1;
        if(key_right!=0 &amp;&amp; !place_meeting(x+1,y,obj_wall) &amp;&amp; place_meeting(x-200,y,box))
        {
            with(box)
            {
    
                x+=1;
            }
        }
        else if(key_left!=0 &amp;&amp; !place_meeting(x-1,y,obj_wall)&amp;&amp; place_meeting(x+200,y,box))
        {
            with(box)
            {
    
                x-=1;
            }
        }      
}
if(pull==0)
movespeed = 4;
</string>
          </argument>
        </arguments>
      </action>
